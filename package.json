{
  "name": "porkbunjs",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript library for the Porkbun API v3",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "porkbun",
    "domain",
    "dns",
    "api",
    "typescript",
    "registrar",
    "domains",
    "dnssec",
    "ssl"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/porkbunjs.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/porkbunjs/issues"
  },
  "homepage": "https://github.com/yourusername/porkbunjs#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.7.5",
    "gts": "^6.0.2",
    "tsdown": "^0.12.9",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "test": "echo 'Tests not implemented yet'",
    "prepublishOnly": "npm run build"
  }
}
